{
  "name": "nutrisap-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "db:migrate": "prisma db push",
    "db:deploy": "node scripts/deploy-db.js",
    "vercel-build": "if [ -z \"$DATABASE_URL\" ]; then cross-env DATABASE_URL=\"postgresql://temp:temp@localhost:5432/temp\" prisma generate; else prisma generate && prisma db push --accept-data-loss; fi && next build"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "framer-motion": "^12.23.5",
    "jose": "^6.0.11",
    "next": "15.3.5",
    "openai": "^5.10.1",
    "prisma": "^6.11.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsx": "^4.20.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
